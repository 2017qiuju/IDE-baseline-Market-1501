net: "models/mars/resnet50/resnet_50.prototxt"

test_iter: 120
test_interval: 10000
test_initialization: false
display: 20
average_loss: 20

base_lr: 0.001
lr_policy: "step"
gamma: 0.1

stepsize: 35000
max_iter: 175000
momentum: 0.9
weight_decay: 0.0005
snapshot: 0
snapshot_prefix: "output/mars_train/resnet_50"
solver_mode: GPU
